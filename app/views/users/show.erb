<div class="container">

  <h1>Hello, <%= @user.username %></h1>

  <h3>Your Agendas</h3>
  <ul>
    <% @user.agendas.each do |agenda| %>
      <li><%= agenda.name %><li>
    <% end %>
  </ul>

  <h3>Your Saved Agendas</h3>
  <ul>
    <%@user.saves.each do |save|%>
      <li><%= Agenda.find(save.agenda_id).name %></li>
    <%end%>
  </ul>


</div>
